<?xml version="1.0" encoding="utf-8"?>
<root title='config' >

    <protocol title='protocol'>
        <name>ip</name>
        <parameter>src</parameter>
        <parameter>dst</parameter>
    </protocol>

    <protocol title='protocol'>
        <name>tcp</name>
        <parameter>srcport</parameter>
        <parameter>dstport</parameter>
        <parameter>window_size</parameter>
        <parameter>window_size_scalefactor</parameter>
        <parameter>window_size_value</parameter>
    </protocol>

    <protocol title='protocol'>
        <name>http</name>
        <parameter>host</parameter>
        <parameter>request</parameter>
        <parameter>response</parameter>
        <parameter>prev_request_in</parameter>
        <parameter>request_in</parameter>
        <parameter>prev_response_in</parameter>
        <parameter>response_in</parameter>
        <parameter>request_full_uri</parameter>
        <parameter>request_method</parameter>
        <parameter>response_code</parameter>
        <parameter>time</parameter>
        <parameter>data_len</parameter>
        <parameter>content_length</parameter>
    </protocol>

   <protocol title='protocol'>
        <name>dns</name>
        <parameter>qry_name</parameter>
        <parameter>qry_type</parameter>
        <parameter>resp_name</parameter>
        <parameter>resp_type</parameter>
        <parameter>resp_ttl</parameter>
        <parameter>resp_len</parameter>
        <parameter>time</parameter>
        <parameter>a</parameter>
    </protocol>

    <protocol title='protocol'>
        <name>mysql</name>
        <parameter>command</parameter>
        <parameter>packet_length</parameter>
        <parameter>packet_number</parameter>
        <parameter>request</parameter>
        <parameter>query</parameter>
        <parameter>schema</parameter>
        <parameter>_ws_expert_message</parameter>
        <parameter>affected_rows</parameter>
        <parameter>stat_ps_out_params</parameter>
        <parameter>stat_query_was_slow</parameter>
        <parameter>stat_session_state_changed</parameter>
    </protocol>

    <protocol title='protocol'>
        <name>pgsql</name>
        <parameter>type</parameter>
        <parameter>length</parameter>
        <parameter>query</parameter>
        <parameter>parameter_name</parameter>
        <parameter>parameter_value</parameter>
        <parameter>statement</parameter>
    </protocol>

    <protocol title='protocol'>
        <name>tds</name>
        <parameter>type</parameter>
        <parameter>length</parameter>
        <parameter>query</parameter>
        <parameter>packet_number</parameter>
    </protocol>
</root>